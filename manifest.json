{
    "name": "FETtec Configurator",
    "short_name": "FETtecESCConfig",
    "description": "Configuration tool for FETtec devices",
    "offline_enabled": true,
    "author": "FETtec",
    "version": "0.1.33",
    "manifest_version": 2,
    "minimum_chrome_version": "40",
    "permissions": [
        "serial",
        "usb",
        "storage",
        "fileSystem",
        "notifications",
        "https://*.github.com/",
        "https://*.githubusercontent.com/",
        "https://*.s3.amazonaws.com/",
        "https://licensing.fettec.net/"
    ],
    "icons": {
        "144": "src/images/fettec-256x256.png"
    },
    "app": {
        "background": {
            "scripts": [
                "src/js/launcher_manifest.js"
            ]
        }
    }
}